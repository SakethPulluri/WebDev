<%- include("header"); -%>

<body>
  <div id="heading" class="box">
    <h1><%=listTitle%></h1>
  </div>
  <div class="box">
    <% for (let item of items) { %>
    <div class="item">
      <input type="checkbox">
      <p><%= item %></p>
    </div>

    <% } %>
    <form class="item" method="post">
      <input type="text" name="newItem" placeholder="New task" autocomplete="off">
      <button type="submit" name="submit">+</button>
    </form>
  </div>
  
<%- include("footer"); -%>
